    "require": {
        "adodb/adodb-php" : "5.21.3",
        "james-heinrich/phpthumb" : "*",
        "econea/nusoap" : "*",
        "zendframework/zend-soap" : "*",
        "ext-json": "*",
        "respect/validation": "^1.1",
        "stefangabos/zebra_pagination": "^2.3",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/pagination": "^8.83",
        "illuminate/database": "^8.83.27",
        "php-http/httplug": "^2.4"
